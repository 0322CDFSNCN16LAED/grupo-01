<!DOCTYPE html>
<html lang="es">
<head>
<%- include("./partials/head.ejs") %>
<link rel="stylesheet" href="/css/home.css">
<link rel="stylesheet" href="/css/register.css">
<link rel="stylesheet" href="/css/home-tablet.css">
<link rel="stylesheet" href="/css/home-desktop.css">
<link rel="stylesheet" href="/css/laptop.css">
</head>

<body class="width-100vw height-100vh font-family-lilita bg-pagina display-flex flex-column min-heigth-100vh">

<header>
  <%- include("./partials/header.ejs") %>

</header>
<body>
    <h2> Listado de Usuarios </h2>
    
    <% users.forEach(user => { %> 
        <a class="display-flex " href="/detalleUser/<%=user.id%>">
            <p class="width-45pc"> <%=user.nombre  %>  </p>
            <p> <%=user.apellido  %>  </p>
        </a>  
    <% }) %> 

     
</body>
<footer>
    <%- include("./partials/footer.ejs") %>

</footer>
</html>
